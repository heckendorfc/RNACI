##########################################################

PKG_CPPFLAGS = @DEFS@
PKG_CFLAGS = -w

USER_CONF = Makeconf


R_OBJS = \
	Rtools/alloc.o \
	Rtools/misc.o \
	Rtools/printing.o \
	Rtools/structures_dataframes.o \
	Rtools/structures_lists.o \
	Rtools/structures_misc.o \
	tests/tests.o \
	package_examples.o


OBJECTS = ${R_OBJS}

all: $(SHLIB)

$(SHLIB): $(OBJECTS)

clean:
		@rm -rf *.o *.d *.rc *.so* *.dylib *.dll *.a *.lib \
				Makedeps Makevars $(SHLIB) $(OBJECTS)

